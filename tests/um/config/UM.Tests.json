{
	"testing":{
		"testsx":"UserTests.*",
		"recreateDB": true
	},
	"clusters": {
		"_":{
			"driver": "$(JDE_DB_DRIVER)",
			"connectionString": "$(JDE_TEST_UM_CONNECTION)",
			"metadata": [
				"$(JDE_DIR)/Public/src/um/config/um-schema.json"
			],
			"qlMetadata": [
				"$(JDE_DIR)/Public/src/um/config/ql.json"
			]
		}
	},
	"logging":{
		"tags": {
			"Trace":["test", "ql"],
			"Debug":["settings", "app"],
			"Information":[],
			"Warning":[],
			"Error":[],
			"Critical":[]
		},
		"console":{}
	},
	"workers":{
		"drive": {"threads": 1}
	}
}